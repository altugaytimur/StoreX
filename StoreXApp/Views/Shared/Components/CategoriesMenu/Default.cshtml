@model IEnumerable<Category>

<div class="mt-3">
    <h6 class="lead">
        <i class="bi bi-list-ul text-secondary"></i>
       Categories
    </h6>
</div>
<div class="my-4">
    <ul class="list-group">
        @foreach (var item in Model)
        {
            <li class="list-group-item">
                <a asp-action="Index" asp-route-CategoryId="@item.Id">
                    @item.CategoryName
                </a>
            </li>
        }
    </ul>
</div>



