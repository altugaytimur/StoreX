@model IEnumerable<Order>
@if(Model.Count()>0)
{
    @foreach (var item in Model)
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@item"/>
        </div>
    }
  @*  <table class="table table-hover">
        <thead>

        </thead>
    </table>*@
}
else
{
    <div class="alert alert-info">
        <p class="lead">There is no order yet.</p>
    </div>
}